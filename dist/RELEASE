# $Id$

# These two release numbers are deprecated as of January 2018
DB_VERSION_FAMILY=18
DB_VERSION_RELEASE=1

# These four lines are updated by dist/bumprel.
DB_VERSION_MAJOR=18
DB_VERSION_MINOR=1
DB_VERSION_PATCH=40
DB_RELEASE_DATE="May 29, 2020"


DB_VERSION="$DB_VERSION_MAJOR.$DB_VERSION_MINOR.$DB_VERSION_PATCH"
DB_VERSION_FULL="$DB_VERSION_MAJOR.$DB_VERSION_MINOR.$DB_VERSION_PATCH"
DB_VERSION_UNIQUE_NAME=`printf "_%d%03d" $DB_VERSION_MAJOR $DB_VERSION_MINOR`

DB_VERSION_STRING="Berkeley DB $DB_VERSION: ($DB_RELEASE_DATE)"
DB_VERSION_FULL_STRING="Berkeley DB Release $DB_VERSION_MAJOR.$DB_VERSION_MINOR, library version $DB_VERSION_FULL: ($DB_RELEASE_DATE)"
